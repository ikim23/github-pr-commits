{
  "manifest_version": 2,
  "name": "github-pr-commits",
  "version": "1.0.0",
  "description": "github-commit",
  "minimum_chrome_version": "60",
  "permissions": ["https://api.github.com/*", "storage"],
  "optional_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
