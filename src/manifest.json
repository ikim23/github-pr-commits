{
  "manifest_version": 2,
  "name": "Github Pull Request Commits",
  "version": "1.0.0",
  "description": "Suggest Pull Request commits white typing",
  "permissions": ["https://api.github.com/*", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
